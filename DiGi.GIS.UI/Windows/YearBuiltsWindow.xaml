<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiGi.GIS.UI.Windows"
        xmlns:Controls="clr-namespace:DiGi.GIS.UI.Controls" x:Class="DiGi.GIS.UI.Windows.YearBuiltsWindow"
        mc:Ignorable="d"
        Title="Year Builts" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <StackPanel DockPanel.Dock="Top">
            <Menu>
                <MenuItem Header="File">
                    <MenuItem x:Name="MenuItem_Load" Header="Load" Click="MenuItem_Load_Click"/>
                    <Separator/>
                    <MenuItem x:Name="MenuItem_Close" Header="Close" Click="MenuItem_Close_Click"/>
                </MenuItem>
                <MenuItem Header="Help">
                    <MenuItem x:Name="MenuItem_About" Header="About" Click="MenuItem_About_Click"/>
                </MenuItem>
            </Menu>

        </StackPanel>
        <Grid DockPanel.Dock="Bottom">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Controls:YearBuiltsControl x:Name="YearBuiltsControl_Main" Grid.Column="1" Grid.Row="1">

            </Controls:YearBuiltsControl>
        </Grid>
    </DockPanel>
</Window>
