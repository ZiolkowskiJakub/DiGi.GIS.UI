<Window x:Class="DiGi.GIS.UI.Application.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiGi.GIS.UI.Application"
        mc:Ignorable="d"
        Title="GIS" Height="190" Width="150" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="Button_Convert" Content="Convert" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="130" Click="Button_Convert_Click"/>
        <Button x:Name="Button_Write" Content="Write" HorizontalAlignment="Center" Margin="0,37,0,0" VerticalAlignment="Top" Width="130" Click="Button_Write_Click"/>
        <Button x:Name="Button_Read" Content="Read" HorizontalAlignment="Center" Margin="0,64,0,0" VerticalAlignment="Top" Width="130" Click="Button_Read_Click"/>
        <Button x:Name="Button_Analyse" Content="Analyse" HorizontalAlignment="Center" Margin="0,91,0,0" VerticalAlignment="Top" Width="130" Click="Button_Analyse_Click"/>
        <Button x:Name="Button_Calculate" Content="Calculate" HorizontalAlignment="Center" Margin="0,118,0,0" VerticalAlignment="Top" Width="130" Click="Button_Calculate_Click"/>
    </Grid>
</Window>
